jQuery(document).ready(function(r){function t(r){var t=r+"=";var e=decodeURIComponent(document.cookie);var a=e.split(";");for(var i=0;i<a.length;i++){var n=a[i];while(n.charAt(0)==" "){n=n.substring(1)}if(n.indexOf(t)==0){return n.substring(t.length,n.length)}}return""}var e=t("affwp_visit_id");var a=t("affwp_affiliate_id");var i=t("affwp_campaign");if(a){var n=awp_track_visit_var.url;if(n.substr(-1)=="/"){n=n.substr(0,n.length-1)}target_urls=r("a[href^='"+n+"']");var u=awp_track_visit_var.referral_variable;r(target_urls).each(function(){current_url=r(this).attr("href");current_url=c(current_url,u,a);if(e){current_url=current_url+"&visit="+e}if(i){current_url=current_url+"&campaign="+i}r(this).attr("href",current_url)})}function c(r,t,e){var a=new RegExp("([?|&])"+t+"=.*?(&|#|$)","i");if(r.match(a)){return r.replace(a,"$1"+t+"="+e+"$2")}var i="",n=-1!==r.indexOf("?")?"&":"?";return-1!==r.indexOf("#")&&(i=r.replace(/.*#/,"#"),r=r.replace(/#.*/,"")),r+n+t+"="+e+i}});